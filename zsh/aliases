alias ll='ls -lah'
alias ga='git add'
alias gc='git commit'
alias gp='git push'

alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index

alias gbcp="git bc-cherry-pick"
alias gbse="git bc-show-eligible master"

alias gwip=" git commit --no-verify -m '[WIP]'"
alias gunwip="git log -n 1 | grep -q -c '[WIP]' && git reset HEAD~1"

alias krusty="ssh yuryn@krusty"

alias lg="lazygit"

alias ni="npm i"
alias ns="npm start"
