alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index

alias ga='git add'
alias gb='git branch'
alias gc='git commit'
alias gcm='git checkout master'
alias gco='git checkout'
alias gd='git diff'
alias gf='git fetch'
alias gl='git log'
alias gpl='git pull'
alias gph='git push'
alias gphu='git push --set-upstream origin HEAD'
alias gr='git reset HEAD~'
alias gs='git status'
alias gsd='git stash drop'
alias gsl='git stash list'
alias gspl='git stash pop'
alias gsph='git stash push'
alias ll='ls -lah'
alias ni='npm i'
alias ns='npm start'
