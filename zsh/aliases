alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index

alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gcm='git checkout master'
alias gco='git checkout'
alias gl='git pull'
alias gp='git push'
alias gs='git status'
alias ll='ls -lah'
alias ni='npm i'
alias ns='npm start'
