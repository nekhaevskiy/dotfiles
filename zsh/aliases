alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index

alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gcm='git checkout master'
alias gco='git checkout'
alias gl='git pull'
alias gp='git push'
alias gpsu='git push -u origin HEAD'
alias gs='git status'
alias gsp='git stash push'
alias gsl='git stash pop'
alias gsd='git stash drop'
alias ll='ls -lah'
alias ni='npm i'
alias ns='npm start'
