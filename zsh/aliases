alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index

alias ga='git add'
alias gb='git branch'
alias gc='git commit'
alias gca='git commit --amend'
alias gcan='git commit --amend --no-edit'
alias gcmw='git commit -m WIP'
alias gcm='git checkout master'
alias gco='git checkout'
alias gd='git diff'
alias gds='git diff --staged'
alias gf='git fetch'
alias gl='git log'
alias gpl='git pull'
alias gph='git push'
alias gphu='git push --set-upstream origin HEAD'
alias grm='git rebase master'
alias gs='git status'
alias gsd='git stash drop'
alias gsl='git stash list'
alias gspl='git stash pop'
alias gsph='git stash push'

alias ll='ls -lah'

alias ni='npm i'
alias ns='npm start'
